{
    "name": "gjc/bootstrap-cms",
    "description": "Bootstrap CMS Is A PHP CMS Powered By Laravel 4.0 With Sentry 2.0",
    "keywords": ["laravel", "framework", "sentry", "bootstrap", "cms", "blog", "login", "Bootstrap CMS", "Bootstrap-CMS", "Graham Campbell", "GrahamCampbell"],
    "license": "GNU Affero General Public License v3",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.3.7",
        "laravel/framework": "4.0.*",
        "pda/pheanstalk": "2.1.*",
        "cartalyst/sentry": "2.0.*",
        "vtalbot/markdown": "1.4.*",
        "kmd/logviewer": "dev-master",
        "fideloper/proxy": "dev-master",
        "jasonlewis/basset": "dev-develop",
        "natxet/CssMin": "dev-master",
        "linkorb/jsmin-php": "dev-master",
        "way/generators": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master",
        "way/laravel-test-helpers": "dev-master"
    },
    "autoload": {
        "classmap": [
            "app/libraries",
            "app/relations",
            "app/models",
            "app/controllers",
            "app/handlers",
            "app/commands",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php",
            "app/tests/models/ModelTestCase.php",
            "app/tests/controllers/ControllerTestCase.php",
            "app/tests/controllers/ResourcefulTestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "develop"
        }
    },
    "minimum-stability": "dev"
}
